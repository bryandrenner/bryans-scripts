{
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80,
        120
    ],
    "editor.tabSize": 2,
    "explorer.openEditors.visible": 0,
    "files.hotExit":"off",
    "git.autofetch": true,
    "window.menuBarVisibility": "hidden",
